{

  "name": "RequestTab",
  "description": "Quickly create RequestBin bins",
  "version": "0.0.5",

  "manifest_version": 2,

  "browser_action": {
    "default_title": "RequestTab",
    "default_popup": "index.html"
  },

  "icons": { "16": "icon16.png",
             "48": "icon48.png",
             "128": "icon128.png" },

  "permissions": [
    "cookies",
    "http://requestb.in/"
  ]
}
